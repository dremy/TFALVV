}

#region-preface-first .views-exposed-form .views-submit-button {
padding:0px;
}

#zone-preface {
min-height: 100px;
background: white;
box-shadow: #404040 0px 0px 25px 0px;
position: relative;
top: 30px;
}

/*ZONE CONTENT*/
#zone-content-wrapper {
box-shadow: #000 0px -20px 50px -30px;
}

#zone-content {
position: relative;
z-index: 3;
background: white;
padding-top:10px;
border-left:1px solid #ccc;
border-right:1px solid #ccc;
}

#region-content {
border-left:1px solid #ccc;
padding-left:15px;
margin-left:0px;
margin-right:0px;
}

